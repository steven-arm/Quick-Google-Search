{
  "manifest_version": 3,
  "name": "InterviewHelper",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "permissions": ["scripting", "activeTab"],
  "commands": {
    "test_command":{
      "suggested_key": {
        "default": "Ctrl+Shift+Z"
      },
      "description": "Test Website Opening"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}